{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "typeRoots": [
            "./node_modules/@types"
        ],
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "declaration": true,
        "jsx": "react-jsx",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "esModuleInterop": true
    },
    "include": [
        "data/*.ts"
    ],
    "ignore": [
        "node_modules"
    ]
}